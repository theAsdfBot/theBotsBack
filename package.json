    "@types/jest": "^26.0.0",
    "@types/node": "^14.0.13",
    "@types/sequelize": "^4.28.9",
    "@types/supertest": "^2.0.9",
    "@types/validator": "^13.0.0",
    "@typescript-eslint/eslint-plugin": "^3.3.0",
